#Multiple plotting graphs
library(readr)
library(dplyr)
library(ggplot2)
library(scales)
data_cummulative_results<-read.csv("Cummulative.sto.results.csv", header = TRUE)
theme_bare <- theme(panel.background = element_blank(),
panel.border = element_blank(),
# axis.title = element_blank(),
axis.ticks = element_blank(),
panel.grid = element_blank())
#choose colors from https://brandcolors.net/
stock_colors <- c("1NN Model Accuracy" = "#FF0000",
"1NN Model Preciseness" = "#1B2924",
"1NN Model Sensitivity" = "#50FF40",
"3NN Model Accuracy" = "#FF933D",
"3NN Model Preciseness" = "#3243FA",
"3NN Model Sensitivity" = "#FFD424"
)
g<- ggplot(data = data_cummulative_results,
aes(x = data_cummulative_results$Threshold, y = data_cummulative_results$Score, color = Ticker)) +
geom_line() + scale_color_manual(values = stock_colors) +
theme(legend.position="top", legend.direction="horizontal") +
scale_fill_discrete("")
g <- g + theme_bare + coord_fixed(ratio = 0.3) +
theme(axis.text = element_text(face = "bold", size = rel(0.8))
,legend.title = element_blank())
g<- g + xlab("Threshold") + ylab("Score")
ggsave("sto.prediction.pdf", plot = g, device = NULL, path = NULL,
scale = 1, width = NA, height = NA, units = c("in", "cm", "mm"),
dpi = 300, limitsize = FALSE)
